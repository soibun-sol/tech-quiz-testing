{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "skipLibCheck": true,
        "types": [
            "cypress"
        ]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        "cypress/component/Quiz.cy.jsx",
        "cypress/e2e/quiz.cy.js"
    ],
    "exclude": [
        "node_modules"
    ]
}